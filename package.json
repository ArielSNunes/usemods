{
  "name": "usemods",
  "version": "0.0.4",
  "description": "Zippy little modifiers and utilities for your frontend.",
  "scripts": {
    "dev": "bun run builder.mjs",
    "build": "bun run build.mjs",
    "test": "bun --watch test test/*",
    "release": "npm run test && changelogen --release && npm publish && git push --follow-tags",
    "prepublishOnly": "bun run build",
    "docs": "concurrently \"npm run docs:install && npm run module:dev\" \"npm run dev\"",
    "docs:install": "npm i --prefix ./nuxt-module && npm i --prefix ./nuxt-module/docs",
    "module:dev": "npm run build && npm run dev --prefix ./nuxt-module",
    "module:release:": "npm run release --prefix ./nuxt-module",
    "vercel:install": "npm i && npm run docs:install",
    "vercel:build": "npm run build && npm run prepack:stub --prefix ./nuxt-module && npm run build --prefix ./nuxt-module/docs"
  },
  "keywords": [
    "bun",
    "usemods",
    "utils",
    "helpers",
    "js",
    "vue",
    "nuxt",
    "react",
    "next"
  ],
  "license": "MIT",
  "homepage": "https://github.com/LittleFoxCompany/usemods#readme",
  "bugs": "https://github.com/LittleFoxCompany/usemods/issues",
  "author": "Jeremy Butler <jeremy@littlefox.studio>",
  "contributors": [
    {
      "name": "Jeremy Butler",
      "email": "jeremy@littlefox.studio",
      "url": "https://github.com/jrmymbtlr"
    },
    {
      "name": "Phillip Hartin",
      "email": "phillip@littlefox.studio",
      "url": "https://github.com/pjhartin"
    }
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LittleFoxCompany/usemods.git"
  },
  "devDependencies": {
    "@nuxtjs/color-mode": "^3.3.2",
    "@types/web": "^0.0.119",
    "@vueuse/core": "^10.7.2",
    "@vueuse/nuxt": "^10.7.2",
    "bun-plugin-dts": "^0.2.1",
    "bun-types": "^1.0.7",
    "changelogen": "^0.5.5",
    "concurrently": "^8.2.2",
    "monaco-editor": "^0.45.0",
    "nuxt": "^3.10.2",
    "nuxt-monaco-editor": "^1.2.7",
    "shikiji": "^0.10.0-beta.6",
    "typescript": "^5.2.2"
  }
}
